# programa con operaciones
programa cuatro:
	constantes
		a = 10;
		b = 20;
	variables
		rimbo : entero;
		resultado : entero;
		
		conjuncion : booleano;
		resultadoLogico : booleano;


comienzo
	rimbo = a + b	;
	resultado = rimbo * 10 + 4;
	
	escribir(rimbo);
	escribir(resultado);

	conjuncion = cierto y falso;
	resultadoLogico = conjuncion y (a < b);
	
	escribir(conjuncion);
	escribir(resultadoLogico);
	
	

fin.
